{
  "name": "agorralab",
  "productName": "AgorraLab",
  "version": "1.0.0",
  "description": "نظام إدارة مختبرات الأسنان",
  "main": "electron/index.js",
  "scripts": {
    "dev": "vite",
    "build:main": "tsc -p tsconfig.main.json",
    "postbuild:renderer": "node scripts/copy-sql-wasm.js",
    "build:renderer": "vite build && npm run postbuild:renderer",
    "build": "npm run build:main && npm run build:renderer",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"cross-env NODE_ENV=development electron .\"",
    "electron": "electron .",
    "electron:build": "npm run build && electron .",
    "rebuild": "npx @electron/rebuild -f -w",
    "postinstall": "electron-builder install-app-deps",
    "test": "echo \"No tests specified\"",
    "install-deps": "npm install vite @vitejs/plugin-react react react-dom typescript",
"prepare:icons": "node scripts/fix-assets-icon.js",
    "dist": "npm run prepare:icons && cross-env NODE_ENV=production npm run build && electron-builder",
    "dist:win": "npm run prepare:icons && rimraf dist-electron && cross-env NODE_ENV=production npm run build && electron-builder --win --x64 --publish never",
    "dist:mac": "npm run prepare:icons && rimraf dist-electron && cross-env NODE_ENV=production npm run build && electron-builder --mac",
    "dist:linux": "npm run prepare:icons && rimraf dist-electron && cross-env NODE_ENV=production npm run build && electron-builder --linux",
    "lint": "eslint --ext .ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "generate-license": "node scripts/generate-license.js"
  },
  "keywords": [],
  "author": {
    "name": "Mohsen-it",
    "email": "mohsen.it.eng@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/pdfkit": "^0.17.3",
    "@types/pino": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.11.0",
    "electron": "39.1.1",
    "electron-builder": "^26.0.12",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jsdom": "^27.2.0",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.18",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "webpack-cli": "^6.0.1",
    "webpack": "^5.88.2",
    "ts-node": "^10.9.1",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@whiskeysockets/baileys": "^6.7.21",
    "arabic-reshaper": "^1.1.0",
    "bidi-js": "^1.0.3",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "exceljs": "^4.4.0",
    "jimp": "^1.6.0",
    "lucide-react": "^0.553.0",
    "pdfkit": "^0.17.2",
    "pino": "^10.1.0",
    "postcss-loader": "^8.2.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^7.9.5",
    "sql.js": "^1.13.0",
    "zod": "^4.1.12"
  },
  "build": {
    "appId": "com.ORalSoft",
    "productName": "AgorraLab",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "src/**/*",
      "assets/**/*",
      "installer/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "installer",
        "to": "installer"
      },
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "asarUnpack": ["**/sql-wasm.wasm"],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "requestedExecutionLevel": "asInvoker",
      "forceCodeSigning": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "perMachine": true,
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico",
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "shortcutName": "AgorraLab",
      "language": "1033",
      "displayLanguageSelector": false,
      "multiLanguageInstaller": false,
      "license": "installer/license-en.txt",
      "artifactName": "AgorraLab-v${version}-Setup.${ext}",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "menuCategory": "Healthcare",
      "warningsAsErrors": false,
      "unicode": true,
      "guid": "AgorraLab-v1.0"
    },
    "mac": {
      "target": "dmg",
      "icon": "installer/icon.icns",
      "category": "public.app-category.healthcare-fitness"
    },
    "linux": {
      "target": "AppImage",
      "icon": "installer/icon.png",
      "category": "Office"
    },
    "publish": null
  }
}
